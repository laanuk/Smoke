{
  "manifest_version": 2,
  "name": "Smoke",
  "version": "0.1",
  "permissions": [
  "history"
  ],
  "background": {
    "scripts": ["background.js", "jquery.min.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
